<!DOCTYPE html>
<html>
<head>
  <title>LayoutsAssignment</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-4"><%= yield :div1 %></div>
            <div class="col-sm-4"><%= yield :div2 %></div>
            <div class="col-sm-4"><%= yield :div3 %></div>
        </div>
    </div>
</body>
</html>